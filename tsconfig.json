{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"isolatedModules": true,
		"incremental": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"allowImportingTsExtensions": true,
		"types": ["vite/client"]
	},
	"include": ["src", "vite.config.ts"]
}
